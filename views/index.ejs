    <div id="menu-tab" class="closed">MENU</div>
  </div>

  <!-- AVATAR in NAV -->
  <section class="topNavLoginBar">
        <img ng-src="{{ main.currentUser.picture }}" class="avatar">
        <div ng-show="main.isLoggedIn()">
        <strong>{{ main.currentUser.name }}</strong>
        <a ng-click="main.logout()"><i class="fa fa-sign-out"></i>(Logout)</a>
    </div>
  </section>

<hr>
